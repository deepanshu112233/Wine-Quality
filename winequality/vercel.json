{
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/app.py/$1"
      },
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ]
  }